{
    "name": "ibnnajjaar/graphify",
    "description": "A simple open graph image generator for Laravel blogs.",
    "keywords": [
        "ibnnajjaar",
        "laravel",
        "graphify"
    ],
    "homepage": "https://github.com/ibnnajjaar/graphify",
    "license": "MIT",
    "authors": [
        {
            "name": "ibnnajjaar",
            "email": "ibnahnajjaar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.2",
        "spatie/browsershot": "^3.0.0",
        "spatie/laravel-package-tools": "^1.14"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Ibnnajjaar\\Graphify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ibnnajjaar\\Graphify\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ibnnajjaar\\Graphify\\GraphifyServiceProvider"
            ],
            "aliases": {
                "Graphify": "Ibnnajjaar\\Graphify\\Facades\\Graphify"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
